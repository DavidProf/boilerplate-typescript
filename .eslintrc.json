{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "indent": ["error", 4],
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "valid-jsdoc": "error"
    }
}
